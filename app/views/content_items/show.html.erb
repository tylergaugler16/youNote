
<div class="container show-container">
	<h1><%= @video.title %></h1>
	<div class="show-video-container container-fluid">
		<iframe width="640" height="360" src="https://www.youtube.com/embed/<%= @video.id %>" frameborder="0" allowfullscreen></iframe>
	</div>
	<div class="description container-fluid">
		<%= @video.description %>
	</div>
	<button type="button" class="btn btn-default btn-block">Create Note</button>
</div>

